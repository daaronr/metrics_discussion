file = {:Users/yosemite/Dropbox/articlesandbibtex/statistical learning and R/example{\_}metaanaysis{\_}finl{\_}literacy{\_}mnsc.2013.1849.pdf:pdf},
isbn = {0025-1909},
issn = {0025-1909},
journal = {Management Science},
keywords = {behavioral economics,consumer behavior,education systems,household finance,public policy},
number = {8},
pages = {1861--1883},
pmid = {25246403},
title = {{Financial Literacy, Financial Education, and Downstream Financial Behaviors}},
