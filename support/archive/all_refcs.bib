journal = {Review of Economic Studies},
keywords = {Altruism,Charitable giving,Prosocial behaviour,Risk preferences},
number = {2},
pages = {587--628},
title = {{Excusing selfishness in charitable giving: The role of risk}},
volume = {83},
year = {2016}
