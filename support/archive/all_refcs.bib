@article{Lankford1991,
abstract = {There is a large literature investigating the determinants of charitable
giving, in particular, price and income elasticities. We find that
many of the most often reported results from this literature rely
on a model specification that is inappropriate when applied to recent
Federal Tax File data. We develop a standard Tobit model with a Box-Cox
transformation parameter that nests a number of other specifications.
