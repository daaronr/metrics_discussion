@article{Levin1975,
abstract = {Attempted to clarify and extend the findings of A. M. Isen and P.
F. Levin's (see record 1972-22883-001) study by examining the effects
of a person's positive affective state on his or her subsequent helpfulness
to others in 2 experiments. The studies were conducted in divergent
locations of a large eastern city, and Ss differed in ethnic and
