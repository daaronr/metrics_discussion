journal = {Ssrn},
keywords = {C91,Charitable Giving,Competition,D64,Experimental Economics,H41,P43,Social Preferences},
title = {{Is Charitable Giving a Zero Sum Game? - The Effect of Competition Between Charities on Giving Behavior}},
