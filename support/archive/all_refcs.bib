isbn = {8585348585},
issn = {1537274X},
journal = {Journal of the American Statistical Association},
keywords = {Efficiency augmentation,Modified covariates,Personalized medicine,Randomized clinical trial,Subgroup analysis},
number = {508},
pages = {1517--1532},
pmid = {25729117},
title = {{A Simple Method for Estimating Interactions Between a Treatment and a Large Number of Covariates}},
