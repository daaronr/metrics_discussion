@techreport{Winter2008,
abstract = {We introduce emotions into an equilibrium notion. In a mental equilibrium
each player -selects- an emotional state which determines the player's
preferences over the outcomes of the game. These preferences typically
differ from the players' material preferences. The emotional states
interact to play a Nash equilibrium and in addition each player's
emotional state must be a best response (with respect to material
preferences) to the emotional states of the others. We first discuss
the concept behind the definition of mental equilibrium and show
that this behavioral equilibrium notion organizes quite well the
results of some of the most popular experiments in the literature
of experimental economics. We expose some attractive properties of
mental equilibria which are useful for deriving the set of mental
equilibria for specific games.},
author = {Winter, Eyal and Garcia-Jurado, Ignacio and Mendez-Naya, Jose Mendez-Naya Luciano},
institution = {Discussion Paper},
keywords = {Behavioral Economics,Emotions,Equilibrium,Keywords: Games,losing face},
