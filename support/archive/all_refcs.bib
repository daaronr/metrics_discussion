@article{Iturbe-Ormaetxea,
abstract = {Gneezy and Potters (1997) run an experiment to test the empirical content of Myopic Loss Aversion (MLA). They find that the attractiveness of a risky asset depends upon investors' time horizon: consistently with MLA, individuals are more willing to take risks when they evaluate their investments less frequently. This paper shows that these experimental findings can be accommodated by a standard version of Expected Utility Theory, namely a CRRA functional, provided that, consistently with MLA, we impose narrow bracketing. We use four different datasets to estimate two CRRA and two MLA functionals, together with binary mixture models of all combinations of the competing models. Our econometric exercise supports our theoretical claim on narrow bracketing and finds little evidence of subjects' loss aversion. Caroline Madden, Andrew Schotter and all people at CESS and Econ NYU for their kind hospitality. Financial support from Ministerio de Econom{\'{i}}a y Competitividad (ECO2012-34928 and ECO2015-65820-P), MIUR (PRIN 20103S5RN3 002), Generalitat Valenciana (Prometeo/2013/037) and Instituto Valenciano de Investigaciones Econ{\'{o}}micas is gratefully ac-knowledged.},
author = {Iturbe-Ormaetxe, I{\~{n}}igo and Ponti, Giovanni and Tom{\'{a}}s, Josefa},
journal = {Working Paper},
keywords = {C91,D14 Keywords,D81,Expected Utility Theory,JEL Classification,Myopic Loss Aversion,Narrow Bracketing},
